<message-types xmlns="werkflow:fundamental"
               xmlns:wfjava="werkflow:java">

  <message-type id="normal.msg">
    <wfjava:selector type="org.codehaus.werkflow.SimpleMessage"
                     filter="message.type.equals( &quot;normal&quot; )"/>
  </message-type>

  <message-type id="init.msg">
    <wfjava:selector type="org.codehaus.werkflow.SimpleMessage"
                     filter="message.type.equals( &quot;init&quot; )"/>
  </message-type>
  
  <!-- -->

  <message-type id="number.msg">
    <wfjava:selector type="java.lang.Number"/>
  </message-type>

</message-types>
